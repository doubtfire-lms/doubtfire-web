{
  "name": "doubtfire",
  "version": "4.0.0",
  "homepage": "http://doubtfire.ict.swin.edu.au",
  "description": "A modern, lightweight learning management system.",
  "license": "AGPL-3.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/doubtfire-lms/doubtfire-web.git"
  },
  "scripts": {
    "build": "npm-run-all clean:prod build:prod",
    "build:dev": "webpack-dev-server --progress --config config/webpack/dev.webpack.conf.js --hot",
    "build:prod": "webpack -p --progress --config config/webpack/prod.webpack.conf.js",
    "build:serve": "npm-run-all build serve:prod",
    "clean:dev": "rimraf dev/",
    "clean:prod": "rimraf dist/",
    "dev": "npm-run-all clean:dev verify --parallel verify:watch build:dev serve:dev",
    "copyenv": "grunt environment",
    "release": "npm-run-all build verify test:unit:once",
    "serve:dev": "echo Starting DEV server",
    "serve:prod": "grunt connect:prod",
    "start": "npm run dev",
    "test": "npm run test:unit",
    "test:browser": "protractor config/testBrowser/protractor.conf.js",
    "test:unit": "karma start ./config/testUnit/karma.conf.js",
    "test:unit:debug": "karma start ./config/testUnit/karma.conf.js --debug",
    "test:unit:once": "karma start --singleRun=true ./config/testUnit/karma.conf.js",
    "verify": "grunt verify",
    "verify:watch": "grunt watch:verify"
  },
