{
  "paths": {
    "config": {
      "configDir": "config/",
      "tempDir": ".tmp/"
    },
    "input": {
      "assetsDir": "assets/",
      "browserTestDir": "browserTest/",
      "modulesDir": "src/modules/",
      "modulesSubDir": "modules/",
      "srcDir": "src/",
      "stylesDir": "styles/",
      "templateDir": "template/",
      "unitTestDir": "unitTest/"
    },
    "output": {
      "assetsSubDir": "assets/",
      "cssSubDir": "css/",
      "devDir": "dev/",
      "jsSubDir": "js/",
      "prodDir": "dist/",
      "reportDir": "reports/",
      "vendorJSSubDir": "vendor/",
      "viewsSubDir": "views/"
    }
  },
  "buildAssets": {},
  "buildCSS": {
    "autoprefixer": true,
    "sourceFormat": "sass"
  },
  "buildJS": {
    "framework": [
      "AngularJS 1.x"
    ],
    "frameworkScripts": [
      "angular"
    ],
    "vendorScripts": [
      "angular-route"
    ]
  },
  "buildHTML": {
    "extension": ".html"
  },
  "build": {},
  "entryPoint": {
    "entryPoints": {
      "sampleApp": [
        "./modules/demoModule/app.js"
      ]
    }
  },
  "serverDev": {
    "hostname": "localhost",
    "port": 8000,
    "protocol": "http"
  },
  "serverProd": {
    "hostname": "localhost",
    "port": 8000,
    "protocol": "http"
  },
  "testUnit": {
    "testDependencies": [
      "angular",
      "angular-mocks",
      "angular-route"
    ]
  }
}
